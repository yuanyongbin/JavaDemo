package cn.itcast_02;

public class Student {
	String name;
	int age;
	public Student(){}
	public Student(String name,int age){
		this.name = name;
		this.age = age;
	}
	
	public void setName(String name){
		this.name = name;
	}
	public String getName(){
		return name;
	}
	public void SetAge(int age){
		this.age = age;
	}
	public int getAge(){
		return age;
	}
}
